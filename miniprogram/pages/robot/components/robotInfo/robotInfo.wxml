<!--pages/robot/components/robotInfo.wxml-->
<view class="robot-info">
  <view class="robot-info-left">
  <!-- 机器人状态 -->
  <!-- 暂用base64 后期修改为服务器 -->
    <view class="robot-info-left-item" style="margin-bottom:16rpx;background-image: url('data:image/png;base64,');">
      <view>
        <view class="robot-info-title">机器人状态</view>
        <view class="robot-info-status">{{taskInfo.robotState===1?'在线':'离线'}}</view>
      </view>
      <text class="iconfont icon-ic_robot_robotstatus" style="font-size: 64rpx;"></text>
    </view>
    <!-- 机器人电量 -->
    <view class="robot-info-left-item" style="background-image: url('data:image/png;base64,');">
      <view>
        <view class="robot-info-title">机器人电量</view>
        <view class="robot-info-status">{{taskInfo.robotBattery}}%</view>
      </view>
      <text class="iconfont icon-ic_robot_robotpower" style="font-size: 64rpx;"></text>
    </view>
  </view>
  <!-- 任务状态 -->
    <view class="robot-info-right" style="background-image: url('data:image/png;base64,');">
      <view>
        <view class="robot-info-title">任务状态</view>
        <view class="robot-info-status">{{taskInfo.taskState===0?'空闲':'任务中'}}</view>
      </view>
      <view class="robot-info-point" wx:if="{{taskInfo.taskState!==0}}">
        <view style="font-size: 24rpx;color: rgba(255, 255, 255, 0.8);">任务点位</view>
        <view style="font-size: 28rpx;color: rgba(255, 255, 255, 0.8);">{{taskInfo.currentDeviceName}}</view>
      </view>
      <view class="robot-info-view" bind:tap="goAnalyse">
        <text class="iconfont icon-ic_robot_view" style="margin-right: 12rpx;"></text>
        查看色谱
      </view>
    </view>
</view>